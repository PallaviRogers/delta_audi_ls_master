SELECT COMMIT_END_DATE ,COMMIT_ORIG_NO_MONTH ,COMMIT_REASON_CODE ,COMMIT_START_DATE ,CTN_SEQ_NO ,DMT_CRYPTO.tokenize(CUSTOMER_BAN,'K1') AS CUSTOMER_BAN ,DMT_CRYPTO.tokenize(CUSTOMER_ID,'K1') AS CUSTOMER_ID ,DEALER_CODE ,EFFECTIVE_DATE ,INIT_ACTIVATION_DATE ,MSID ,DMT_CRYPTO.tokenize(NEXT_BAN,'K1') AS NEXT_BAN ,NEXT_BAN_MOVE_DATE ,DMT_CRYPTO.tokenize(NEXT_CTN,'K2') AS NEXT_CTN ,NEXT_CTN_CHG_DATE ,PAYEE_IND ,DMT_CRYPTO.tokenize(PRV_BAN,'K1') AS PRV_BAN ,PRV_BAN_MOVE_DATE ,DMT_CRYPTO.tokenize(PRV_CTN,'K1') AS PRV_CTN ,PRV_CTN_CHG_DATE ,SUB_BILL_LANG ,SUB_FEATURE_LANG ,DMT_CRYPTO.tokenize(SUB_GENDER,'K3') AS SUB_GENDER ,SUB_STATUS ,SUB_TYPE ,DMT_CRYPTO.tokenize(SUBSCRIBER_NO,'K2') AS SUBSCRIBER_NO ,WIFI_IND,GG_COMMIT_TS,GG_OP_TYPE,PRODUCT_TYPE,  trim(DECODE(SUB_STATUS,'C',SUB_STS_ISSUE_DATE,NULL)) EXPIRATION_DATE, trim(DECODE(SUB_STATUS,'C',SUB_STS_ISSUE_DATE,NULL)) DEACTIVATION_DATE FROM ELA_V21.SUBSCRIBER
