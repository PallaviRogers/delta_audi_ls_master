								}
							}
						},
						{
							"name": "target_folder",
							"description": "@activity('Get Metadata1').output.childItems",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "target_folder",
								"value": {
									"value": "@concat(pipeline().parameters.target_folder,item().name)",
